{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Next.js Server side",
      "type": "node",
      "request": "attach",
      "port": 9229
    },
    {
      "name": "Next.js Client side",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3001",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack://_N_E/*": "${webRoot}/*"
      }
    }
  ]
}
